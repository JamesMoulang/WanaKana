{
  "presets": [
    [
      "env",
      {
        "loose": false,
        "modules": false
      }
    ]
  ],
  "env": {
    "test": {
      "presets": ["env"]
    },
    "es": {
      "presets": [
        [
          "env",
          {
            "debug": true,
            "useBuiltIns": "usage",
            "targets": {
              "browsers": ["last 2 versions", "> 1%", "not IE < 11"],
              "node": "8.9"
            },
            "exclude": ["transform-regenerator"]
          }
        ]
      ]
    },
    "rollup": {
      "presets": [
        [
          "env",
          {
            "modules": false,
            "debug": true,
            "useBuiltIns": "entry",
            "targets": {
              "browsers": ["last 2 versions", "> 1%", "not IE < 11"]
            },
            "exclude": ["transform-regenerator"]
          }
        ]
      ],
      "plugins": ["external-helpers"]
    }
  }
}
